<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="vms_tax_group" model="account.tax.group">
            <field name="name">VMS taxes</field>
            <field name="sequence">10</field>
        </record>

        <record id="tax_demo_stt" model="account.tax">
            <field name="name">STT</field>
            <field name="amount">10</field>
            <field name="amount_type">percent</field>
            <field name="type_tax_use">sale</field>
            <field name="active">True</field>
            <field name="invoice_label">E</field>
            <field name="description">E</field>
            <field name="tax_group_id" ref="vms_tax_group"/>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record id="tax_demo_vat" model="account.tax">
            <field name="name">VAT</field>
            <field name="amount">9</field>
            <field name="amount_type">percent</field>
            <field name="type_tax_use">sale</field>
            <field name="active">True</field>
            <field name="invoice_label">A</field>
            <field name="description">A</field>
            <field name="tax_group_id" ref="vms_tax_group"/>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record id="tax_demo_ecall" model="account.tax">
            <field name="name">ECALL</field>
            <field name="amount">6</field>
            <field name="amount_type">percent</field>
            <field name="type_tax_use">sale</field>
            <field name="active">True</field>
            <field name="invoice_label">F</field>
            <field name="description">F</field>
            <field name="tax_group_id" ref="vms_tax_group"/>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record id="tax_demo_pb" model="account.tax">
            <field name="name">PB</field>
            <field name="amount">10</field>
            <field name="amount_type">fixed</field>
            <field name="type_tax_use">sale</field>
            <field name="active">True</field>
            <field name="invoice_label">P</field>
            <field name="description">P</field>
            <field name="tax_group_id" ref="vms_tax_group"/>
            <field name="company_id" ref="base.main_company"/>
        </record>

    </data>
</odoo>

